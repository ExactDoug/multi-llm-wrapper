+            end_memory = psutil.Process().memory_info().rss
+            
+            metrics = {
+                "operation": operation_name,
+                "duration": end_time - start_time,
+                "memory_delta": end_memory - start_memory,
+                "status": status,
+                "timestamp": start_time
+            }
